<p id="notice"><%= notice %></p>

<h1>Listing Names</h1>

<table>
<thead>
<tr>
<th>First name</th>
<th>Count</th>
<th colspan="3"></th>
</tr>
</thead>

<tbody>
<% @names.each do |name| %>
    <tr>
    <td><%= name.first_name %></td>
    <td><%= name.first_name_count %></td>
    <td><%= link_to 'Edit', edit_name_path(name.id) %></td>
    </tr>
    <% end %>
</tbody>
</table>

<%= link_to 'Back', names_url %>

<br>
